{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es2022",
      "target": "es2017",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "downlevelIteration": true
    },
    "exclude": [
      "**/node_modules",
      "**/build",
      "**/page"
    ]
  }